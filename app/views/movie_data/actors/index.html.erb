<header class="actor-index-header">
  <h1>Listing Actors</h1>
</header>

<br>


<br>

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @actors.each do |actor| %>
      <tr>
        <td><%= link_to actor.name, actor_path(actor) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= will_paginate @actors %>
